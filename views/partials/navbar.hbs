<nav class="navbar border-bottom navbar-expand-lg py-4 bg-light">
  <div class="container">
    <a class="navbar-brand" href="#">
        <img src="{{data.brandIconUrl}}" alt="Logo" width="40" height="36" style="border-radius: 50%;" class="d-inline-block align-text-top">
        {{data.brandName}}</a>
    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav ms-auto mb-2 mb-lg-0">

        {{!-- for all menu use loop by each --}}
        {{#each data.links}}
        <li class="nav-item">
          <a class="nav-link active" aria-current="page" href={{this.url}}>{{this.label}}</a>
        </li>
        {{!-- to stop or close the loop --}}
        {{/each}}
      </ul>
    </div>
  </div>
</nav>